{
	"env": {
		"browser": false,
		"es6": true,
		"node": true
	},
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 11,
		"sourceType": "module"
	},
	"rules": {
		"no-var": "error",
		"no-multiple-empty-lines": ["error",{ "max": 1, "maxEOF": 0, "maxBOF": 0 }],
		"prefer-const": "error"
	}
}